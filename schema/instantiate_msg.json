{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InstantiateMsg",
  "type": "object",
  "required": [
    "bank_contract_address",
    "betting_deadline_height",
    "denom",
    "fee_late",
    "minimum_amount",
    "price"
  ],
  "properties": {
    "bank_contract_address": {
      "type": "string"
    },
    "betting_deadline_height": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "denom": {
      "type": "string"
    },
    "fee_late": {
      "type": "integer",
      "format": "uint8",
      "minimum": 0.0
    },
    "minimum_amount": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "price": {
      "type": "string"
    }
  }
}
